{
  "version": 2,
  "name": "smart-home-energy-simulator",
  "framework": null,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VUE_APP_DEMO_MODE": "true",
    "NPM_CONFIG_PRODUCTION": "false"
  },
  "build": {
    "env": {
      "NPM_CONFIG_PRODUCTION": "false"
    }
  },
  "regions": ["iad1"]
}
